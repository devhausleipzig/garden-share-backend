model Message {
    identifier             String       @id @default(uuid())
}

model Reaction {
    identifier             String       @id @default(uuid())
}

model Booking {
    identifier             String       @id @default(uuid())
}

/////////////////
/////////////////

enum Role {
  ADMIN
  USER
}

model User {
  identifier             String       @id @default(uuid())
    createdAt      DateTime    @default(now())
  updatedAt      DateTime    @updatedAt
  role           Role        @default(USER)
  email          String
  firstName      String
  lastName       String
  password       String
  profilePicture String?
  messages       Message[]
  reactions      Reaction[]
  bookings       Booking[]
}